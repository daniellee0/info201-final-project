# Create a variable for the API's base URI
base.uri <- "https://api.propublica.org/congress/v1/"
# Use the endpoint for recent bills on senate, the base URI, and an
# API key to send a GET request to this endpoint
bill.resource <- "115/senate/bills/introduced.json"
bill.response <- GET(paste0(base.uri, bill.resource),
add_headers('X-API-Key' = APIkey))
# Convert the body from JSON into a data frame
bill.body <- fromJSON(content(bill.response, "text"))
# Flatten the recent bills content into a data frame
recent.bills <- flatten(as.data.frame(bill.body$results$bills))
# Create a new data frame for the top 5 recent bills on senate
top.five.bills <- recent.bills[1:5, ] %>% select(bill_id, short_title, committees,
active, latest_major_action,
congressdotgov_url)
# Use the endpoint for recent votes on senate, the base URI,
# and an API key to send a GET request to this endpoint
vote.resource <- "senate/votes/recent.json"
vote.response <-  GET(paste0(base.uri, vote.resource),
add_headers('X-API-Key' = APIkey))
# Convert the body from JSON into a data frame
vote.body <- fromJSON(content(vote.response, "text"))
# Flatten the recent votes content into a data frame
recent.votes <- flatten(as.data.frame(vote.body$results$votes))
# Create a new data frame for the top 5 recent votes on senate with relevant
# columns
top.five.votes <- recent.votes[1:5, ] %>% select(roll_call, description,
vote_type, result,
democratic.majority_position,
republican.majority_position,
url)
# Use the endpoint for education subject, the base URI, and an API key to send
# a GET request to this endpoint
topic.resource <- "bills/search.json?query=education"
topic.response <- GET(paste0(base.uri, topic.resource),
add_headers('X-API-Key' = APIkey))
# Convert the body from JSON into a data frame
topic.body <- fromJSON(content(topic.response, "text"))
# Flatten the bills on education content into a data frame
recent.topic <- flatten(as.data.frame(topic.body$results$bills))
# Create a new data frame for the top 3 recent education bills with relavent
# columns
top.three.topic <- recent.topic[1:3, ] %>% select(bill_id, short_title,
committees, introduced_date,
latest_major_action_date,
latest_major_action,
congressdotgov_url)
# Choose one specific bill to analyze
chosen.bill <- filter(recent.topic, bill_id == "hr4508-115")
# Use the endpoint for related bills, the base URI, and an API key to send
# a GET request to this endpoint
related.resource <- "115/bills/hr4508/related.json"
related.response <- GET(paste0(base.uri, related.resource),
add_headers('X-API-Key' = APIkey))
# Convert the body from JSON into a data frame
related.body <- fromJSON(content(related.response, "text"))
# Flatten the related bills content into a data frame
related.bills <- flatten(as.data.frame(related.body$results$related_bills))
# Create a new data frame with only the relavent columns
related.table <- select(related.bills, bill_id, short_title, relationship,
committees, introduced_date, latest_major_action_date,
latest_major_action, congressdotgov_url)
# Use the endpoint for house members, the base URI, and an API key to send
# a GET request to this endpoint
representative.resource <- "members/house/WA/current.json"
representative.response <- GET(paste0(base.uri, representative.resource),
add_headers('X-API-Key' = APIkey))
# Convert the body from JSON into a data frame
representative.body <- fromJSON(content(representative.response, "text"))
# Flatten the members content into a data frame
representative <-flatten(as.data.frame(representative.body$results))
# Select only the row regards to representative
my.rep <- representative[representative$first_name =="Adam" &
representative$last_name =="Smith", ]
# Store representative's id into a variable
representative.id <- my.rep$id
# Use the endpoint for sponsored bills, the base URI, and an API key to send
# a GET request to this endpoint
sponsor.resource <- paste0("members/", representative.id,
"/bills/introduced.json")
sponsor.response <- GET(paste0(base.uri, sponsor.resource),
add_headers('X-API-Key' = APIkey))
# Convert the body from JSON into a data frame
sponsor.body <- fromJSON(content(sponsor.response, "text"))
# Flatten the sponsored bills content into a data frame and select only
# the relevant rows
sponsor <- flatten(as.data.frame(sponsor.body$results$bills)) %>%
select(bill_id, short_title, primary_subject,
introduced_date, congressdotgov_url)
# Use the endpoint for cosponsored bills, the base URI, and an API key to
# send a GET request to this endpoint
cosponsor.resource <- paste0("members/", representative.id,
"/bills/cosponsored.json")
cosponsor.response <- GET(paste0(base.uri, cosponsor.resource),
add_headers('X-API-Key' = APIkey))
# Convert the body from JSON into a data frame
cosponsor.body <- fromJSON(content(cosponsor.response, "text"))
# Flatten the cosponsored bills content into a data frame and select
# only the relavent rows
cosponsor <- flatten(as.data.frame(cosponsor.body$results$bills)) %>%
select(bill_id, short_title, primary_subject,
cosponsored_date, congressdotgov_url)
# Use the endpoint for member's recent votes, the base URI, and an API key to
# send a GET request to this endpoint
position.resource <- paste0("members/", representative.id, "/votes.json")
position.response <- GET(paste0(base.uri, position.resource),
add_headers('X-API-Key' = APIkey))
# Convert the body from JSON into a data frame
position.body <- fromJSON(content(position.response, "text"))
# Flatten the cosponsored bills content into a data frame and convert the
# "roll call" column to numeric
representative.position <-flatten(as.data.frame(position.body$results$votes,
stringsAsFactors = FALSE))
representative.position$roll_call <- as.numeric(representative.position$roll_call)
# Use the endpoint for house's recent votes, the base URI, and an API key to
# send a GET request to this endpoint
house.resource <- "house/votes/recent.json"
house.response <- GET(paste0(base.uri, house.resource),
add_headers('X-API-Key' = APIkey))
# Convert the body from JSON into a data frame
house.body <- fromJSON(content(house.response, "text"))
# Flatten the house's recent votes content into a data frame
house.majority <- flatten(as.data.frame(house.body$results$votes,
stringsAsFactors = FALSE))
# Store the representative's party into a variable
if (my.rep$party == "D") {
rep.party <- "democratic"
} else if (my.rep$party == "R") {
rep.party <- "republican"
} else {
rep.party <- "independent"
}
# Combine the representative's recent votes with that of the house
# and store into a new data frame
merged.party.vote <- inner_join(representative.position, house.majority,
by='roll_call') %>%
select(position, paste0(rep.party,".majority_position"))
# Calculate the percentage of recent votes where the representative voted
# with the majority of his party
count <- 0
for (call in 1:20) {
if (merged.party.vote[call, 1] == merged.party.vote[call, 2]) {
count <- count + 1
}
}
same.position <- round(count / 20 * 100)
View(top.five.votes)
View(top.five.votes)
View(recent.votes)
View(recent.votes)
